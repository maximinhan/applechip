<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<property name="LOG_FILE" value="${user.home}/.applechip/log/applechip.log" />
	<include resource="org/springframework/boot/logging/logback/base.xml" />
	<logger name="org.springframework" level="WARN" />
	<logger name="com.applechip" level="DEBUG" />
	<logger name="org.hibernate.tool" level="DEBUG" />
	<logger name="org.hibernate.tool.hbm2ddl.TableMetadata" level="DEBUG" />
	<logger name="java.sql.DatabaseMetaData" level="DEBUG" />
	<logger name="org.hibernate.type.descriptor.sql.BasicBinder" level="TRACE" />
</configuration>